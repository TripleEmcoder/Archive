#!/bin/sh
OFFLINES="10 10 50"
TASKS="50 1 50"
REPEAT="1 1 15"
TOOLS=`dirname $0`

for offlines in `seq $OFFLINES`; do
for tasks in `seq $TASKS`; do
for repeat in `seq $REPEAT`; do
	file="$offlines-$tasks-$repeat"
	if [ ! -f $file.in ]; then
		echo "Regenerating $file..." >&2
		$TOOLS/generator $offlines $tasks > $file.in
		echo tag,date,tabus,counts,resets,distance,cmax,time > $file.csv
	fi
done
done
done
